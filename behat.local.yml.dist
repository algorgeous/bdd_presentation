# behat.local.yml.dist
# Do not edit unless you know what you are doing.
# Copy this to behat.local.yml then edit.
default:
  extensions:
    Behat\MinkExtension\Extension:
      base_url: 'http://drupal7.local'
      goutte: ~
      selenium2: ~
      javascript_session: 'selenium2'
    Drupal\DrupalExtension\Extension:
      blackbox: ~
      api_driver: 'drupal'
      drupal:
        drupal_root: '/Users/USERNAME/Sites/drupal7'
